#: ../nact/nautilus-actions-config.glade.h:5
#, no-c-format
msgid ""
"<b>%M</b> : list of the selected files/directories with their full path "
"separated by space."
msgstr ""
"<b>%M</b> : lista de archivos/directorios seleccionados con su ruta completa separados por "
"un espacio."

#: ../nact/nautilus-actions-config.glade.h:7
#, no-c-format
msgid "<b>%U</b> : username of the gnome-vfs URI"
msgstr "<b>%U</b> : usuario del URI de gnome-vfs"

#: ../nact/nautilus-actions-config.glade.h:9
#, no-c-format
msgid "<b>%d</b> : base dir of the selected file(s)"
msgstr "<b>%d</b> : directorio base de los archivo(s) seleccionado(s)"

#: ../nact/nautilus-actions-config.glade.h:11
#, no-c-format
msgid "<b>%f</b> : the name of the selected file or the 1st one if many are selected"
msgstr ""
