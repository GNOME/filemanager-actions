<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<nautilus-actions-config version="0.1">
  <action name="">
    <test>
      <basename>*</basename>
      <isfile>TRUE</isfile>
      <isdir>TRUE</isdir>
      <scheme>
        <type>file</type>
        <type>sftp</type>
        <type>smb</type>
      </scheme>
      <accept-multiple-files>TRUE</accept-multiple-files>
    </test>
    <command>
      <path>/usr/bin/my_prog</path>
      <parameters>my_param</parameters>
    </command>
    <menu-item>
      <label>My Menu Label</label>
      <tooltip>My Menu Label Tooltip</tooltip>
    </menu-item>
  </action>
</nautilus-actions-config>
